<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Goguma.Screen"
             xmlns:scr="clr-namespace:Goguma.Screen" x:Class="Goguma.Screen.SubScreen"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Focusable="False"
             BorderThickness="0" Loaded="SubScreen_OnLoaded">
  <Grid>
    <GroupBox x:Name="borderBox" Margin="5,5,5,5" Header="asd" FontFamily="{StaticResource Galmuri11}" Foreground="White">
      <scr:Screen x:Name="screen" Margin="0,0,0,0"/>
    </GroupBox>
    

  </Grid>
</UserControl>
